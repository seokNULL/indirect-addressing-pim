export LD_LIBRARY_PATH
/usr/synopsys/vcs_vK-2015.09-SP1/linux64/bin/vcsfind_create_index.exe  -z  simv.daidir/debug_dump/fsearch/idents_VcQ8Qt.xml.gz  -o simv.daidir/debug_dump/fsearch/fsearch.db_tmp
\mv simv.daidir/debug_dump/fsearch/fsearch.db_tmp simv.daidir/debug_dump/fsearch/fsearch.db
